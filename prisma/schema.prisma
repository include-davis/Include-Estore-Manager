generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id                   String  @id @default(uuid())
  name                 String
  price                Int
  description          String
  details              String
  weight               Int
  height               Int
  width                Int
  depth                Int
  special_label_needed Boolean
}
